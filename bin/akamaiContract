
const Tool = require('tool');
const Noi = require('noi');
const { Contract } = Noi;

function print_id( id ) {
    console.log( "Contract: " + id );
}

function per_contract( dto ) {
    dto.map( print_id );
}

var commands = {
    list: function ( dto ) {
        Contract.contracts( per_contract );
    }
};

var tool = new Tool( commands );

tool.evaluate( process.argv );

